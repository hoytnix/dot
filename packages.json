{
    "bash": [
        {
            "remote": "bashrc",
            "local": "~/.bashrc"
        }
    ],

    "compton": [
        {
            "remote": "compton.conf",
            "local": "~/.config/compton.conf"
        }
    ],

    "conky": [
        {
            "remote": "conkyrc",
            "local": "~/.conkyrc"
        }
    ],

    "openbox": "~/.config/openbox",

    "terminator": "~/.config/terminator",

    "tint2": "~/.config/tint2",

    "weather": [
        {
            "remote": "run.py",
            "local": "/opt/weather/run.py"
        },
        {
            "remote": "weather.service",
            "local": "/usr/lib/systemd/system/weather.service"
        }
    ]
}