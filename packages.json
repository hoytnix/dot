{
    "compton": [
        {
            "remote": "compton.conf",
            "local": "~/.config/compton.conf"
        }
    ],

    "openbox": "~/.config/openbox",

    "scripts": "~/Bin/scripts",

    "tint2": "~/.config/tint2",

    "vim": [
        {
            "remote": "vimrc",
            "local": "~/.vimrc"
        },
        {
            "remote": "seahorse.vim",
            "local": "~/.vim/colors/seahorse.vim"
        }
    ],

    "weather": [
        {
            "remote": "run.py",
            "local": "/opt/weather/run.py"
        },
        {
            "remote": "weather.service",
            "local": "/usr/lib/systemd/system/weather.service"
        }
    ],

    "zsh": [
        {
            "remote": "zshrc",
            "local": "~/.zshrc"
        },
        {
            "remote": "seahorse.zsh-theme",
            "local": "~/.oh-my-zsh/themes/seahorse.zsh-theme"
        }
    ]
}
